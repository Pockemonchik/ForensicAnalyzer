<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_overview"
    app:itemIconSize="50dp">


    <fragment
        android:id="@+id/navigation_detail_report"
        android:name="com.example.forensicanalyzer.ui.overview.detail_report.DetailReportFragment"
        android:label="@string/title_overview"
        tools:layout="@layout/fragment_detail_report" />
    <fragment
        android:id="@+id/navigation_overview"
        android:name="com.example.forensicanalyzer.ui.overview.OverviewFragment"
        android:label="@string/title_overview"
        tools:layout="@layout/fragment_overview" />
    <fragment
        android:id="@+id/navigation_report"
        android:name="com.example.forensicanalyzer.ui.report.ReportFragment"
        android:label="@string/title_report"
        tools:layout="@layout/fragment_report" />

    <fragment
        android:id="@+id/navigation_cloud"
        android:name="com.example.forensicanalyzer.ui.cloud.CloudFragment"
        android:label="@string/title_cloud"
        tools:layout="@layout/fragment_cloud" />
</navigation>